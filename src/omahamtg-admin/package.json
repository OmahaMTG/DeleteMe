{
  "name": "omaha-mtg-admin",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "antd": "^3.24.2",
    "axios": "^0.19.0",
    "constate": "^1.3.2",
    "faker": "^4.1.0",
    "mitt": "^1.2.0",
    "modali": "^1.2.0",
    "node-sass": "^4.12.0",
    "npm-check": "^5.9.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-mde": "^7.6.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "showdown": "^1.9.0",
    "vsts-npm-auth": "^0.37.0"
  },
  "scripts": {
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:integration": "jest --config jest.config.integration.js",
    "start": "npm run start:dev",
    "types": "typesync",
    "prettier": "prettier --check ./src/**/**.{ts,tsx}",
    "prettier:write": "prettier --write ./src/**/**.{ts,tsx}",
    "citest": "set CI=true&&set JEST_JUNIT_OUTPUT_DIR=coverage&&npm test -- --coverage --reporters=jest-junit",
    "lint": "eslint --ext .ts --ext .tsx src --max-warnings 0",
    "test:coverage": "npm test -- --coverage",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "npm:auth": "vsts-npm-auth -config .npmrc",
    "npm:update": "npm-check --update"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "src/4_Managers/**.*.ni"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": "error"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@testing-library/jest-dom": "^4.1.1",
    "@testing-library/react": "^9.3.0",
    "@testing-library/react-hooks": "^2.0.3",
    "@types/bootstrap": "^4.3.1",
    "@types/jest": "^24.0.18",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.7.11",
    "@types/node-sass": "^4.11.0",
    "@types/prettier": "^1.18.3",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-test-renderer": "^16.9.0",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "@types/react-window-infinite-loader": "^1.0.1",
    "@types/showdown": "^1.9.3",
    "@types/storybook__react": "^4.0.2",
    "@types/testing-library__react": "^9.1.2",
    "@types/testing-library__react-hooks": "^2.0.0",
    "husky": "^3.0.8",
    "jest-junit": "^8.0.0",
    "nock": "^11.4.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-test-renderer": "^16.10.2",
    "typescript": "^3.6.3",
    "typesync": "^0.6.1"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "cobertura",
      "jest-junit"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
